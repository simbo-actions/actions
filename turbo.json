{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["clean:dist", "^build"],
      "outputs": ["dist/**"],
      "outputLogs": "new-only"
    },
    "bundle": {
      "dependsOn": ["build", "^bundle"],
      "outputs": ["dist/**"],
      "outputLogs": "new-only"
    },
    "check": {
      "dependsOn": ["^check"],
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputLogs": "none"
    },
    "clean:node_modules": {
      "cache": false,
      "outputLogs": "none"
    },
    "clean:turbo": {
      "cache": false,
      "outputLogs": "none"
    },
    "clean:dist": {
      "cache": false,
      "outputLogs": "none"
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"],
      "outputLogs": "new-only"
    }
  }
}
